{
  "File": {
    "title": "Read File",
    "description": "Reads a file with options",
    "minOccurs": 1,
    "schema": {
      "type": "object",
      "properties": {
        "filename": {
          "title": "Enter filename",
          "type": "string",
          "pattern": "[a-zA-Z]:[\\\\\\/](?:[a-zA-Z0-9]+[\\\\\\/])*([a-zA-Z0-9]+\\.(tiff|bmp|jpg|jpeg))"
        },
        "minimize": {
          "title": "Minimize file",
          "type": "boolean"
        },
        "uglify": {
          "title": "Uglify file",
          "type": "boolean"
        }
      },
      "required": [
        "filename"
      ]
    }
  }
}

